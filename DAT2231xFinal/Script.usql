
// Create database.
CREATE DATABASE IF NOT EXISTS retail;

// Create the schema.
CREATE SCHEMA IF NOT EXISTS sales;

// Create table for the stock.
CREATE TABLE sales.stock
(id int,
item string);

//Load the data into the stock table.
USE DATABASE retail;
@stock = EXTRACT id int,
item string
FROM "stock.txt"
USING Extractors.Text(' ');

// Create a CSV of chocolate related products
@choco =
    EXTRACT id int, item string
            FROM sales.stock